    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".plan.PlanMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".plan.EditPlanActivity"></activity>     
        <activity android:name=".plan.PreviewPlanActivity"></activity>
        <activity android:name=".plan.PlanMapFullScreenActivity"></activity>
        <activity android:name=".plan.setposition.SetPositionActivity">
        </activity>
    </application>

</manifest>
